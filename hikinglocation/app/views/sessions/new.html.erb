<div class="center">
  <h1>Webb API</h1>
  <h2>Sign In</h2>
</div>

<div>
  <div>
  <%= form_for(:session, method: "post") do |f| %>
    <%= f.label :username  %>
    <%= f.text_field :username, placeholder: "Username" %>

    <%= f.label :password %>
    <%= f.password_field :password, placeholder: "Password" %>

    <%= f.submit "Log In" %>
    <%= link_to "Register", users_new_path %>
  <% end %>
  </div>
</div>